# Git aliases
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'
alias gci='git ci -m '
alias gp='git push -u origin master'

# Remap rm to use trash, safety net against being very sleepy
alias rm=trash

# Remap vim and vi commands to always run neovim(nvim)
alias vim=nvim
alias vi=nvim
